    <script>
        // When document fires ready event
        $(document).ready(function () {

            // Load our template
            var templateSource = $("#list-item-template").html();
            var template = Handlebars.compile(templateSource);

            // Event for submitting
            var clickEvent = function (list, event) {
                event.preventDefault();

                // Get item list text, and clear the text box
                var itemListText = $(event.currentTarget[0]).val();
                $(event.currentTarget[0]).val("");

                // Fill out our template
                var context = {
                    listText: itemListText
                }

                var html = template(context);

                // Append the template
                $(list).append(html);
            }

            // Handle form submissions
            $('.good-form').submit(function (event) {
                // Pass in two parameters, the list and the event
                clickEvent('.good-list', event);
            })

            $('.bad-form').submit(function (event) {
                // Pass in two parameters, the list and the event
                clickEvent('.bad-list', event);
            })


            // Manage deletions, with event delegation
            $('ul').on('click', 'li a', function (event) {
                event.preventDefault();

                // We want to remove the LI element
                $(this.parentElement.parentElement).remove();
            });

        })
    </script>
